entero TIEMPOS, TIEMPOM, TIEMPOA, TIEMPOD, DIASM, DIASA
INICIO
ESCRIBA "INGRESE EL TIEMPO QUE EL ATLETA RECORRE EN UN DIA: "
LEA TIEMPOD
DIASM <- 30
DIASA <- 365

TIEMPOS <- TIEMPOD * 5
TIEMPOM <- TIEMPOS * DIASM
TIEMPOA <- TIEMPOM * DIASA

ESCRIBA "EL TIEMPO PROMEDIO POR SEMANA ES: ", TIEMPOS
LLAMAR NUEVA_LINEA
ESCRIBA "EL TIEMPO PROMEDIO POR MES ", TIEMPOM
LLAMAR NUEVA_LINEA
ESCRIBA "EL TIEMPO PROMEDIO POR AÑO: ", TIEMPOA
LLAMAR NUEVA_LINEA
FIN