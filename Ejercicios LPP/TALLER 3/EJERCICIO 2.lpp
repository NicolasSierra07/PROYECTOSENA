Entero A, B, R, S, M, D
INICIO
 ESCRIBA "EL VALOR DE A: "
 LEA A
 ESCRIBA "EL VALOR DE B: "
 LEA B
 R <- A - B
 S <- A + B
 M <- A * B
 D <- A / B
 ESCRIBA "EL VALOR DE LA RESTA ES: ", R
 LLAMAR NUEVA_LINEA
 ESCRIBA "EL VALOR DE LA SUMA ES: ", S
 LLAMAR NUEVA_LINEA
 ESCRIBA "EL VALOR DE LA MULTIPLICACION ES: ", M
 LLAMAR NUEVA_LINEA
 ESCRIBA "EL VALOR DE LA DIVISION ES: ", D
FIN
 