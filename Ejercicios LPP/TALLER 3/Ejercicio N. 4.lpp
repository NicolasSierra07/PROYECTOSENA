Entero EDJUAN, EDALBER, EDANA, EDMAMA
INICIO
 ESCRIBA "LEA LA EDAD DE JUAN: "
 LEA EDJUAN
 LLAMAR NUEVA_LINEA
 EDALBER <- 2 * EDJUAN/3
 EDANA <- 4 * EDJUAN/3
 ESCRIBA "LA EDAD DE ALBERTO ES: ", EDALBER
 LLAMAR NUEVA_LINEA
 ESCRIBA "LA EDAD DE ANA ES: ", EDANA
 LLAMAR NUEVA_LINEA
 EDMAMA <-EDJUAN + EDALBER + EDANA
 ESCRIBA "EDAD DE MAMA ES ", EDMAMA
FIN

 