cadena [25] NOMBRE
entero SALARIOH, SALARIOM, HORAS
INICIO
 ESCRIBA "NOMBRE DEL EMPLEADO: "
 LEA NOMBRE
 ESCRIBA "SALARIO DEL EMPLEADO POR HORAS: "
 LEA SALARIOH
 ESCRIBA "NUMERO DE HORAS TRABAJADAS: "
 LEA HORAS
 SALARIOM <- SALARIOH * HORAS
 SI SALARIOM > 450000 entonces
  ESCRIBA "EL NOMBRE DEL EMPLEADO ES: ", NOMBRE
  LLAMAR NUEVA_LINEA
  ESCRIBA "EL SALARIO DEL EMPLEADO ES: ", SALARIOM
 SINO 
  ESCRIBA "EL NOMBRE DEL EMPLEADO ES: ", NOMBRE
 FIN SI
FIN
 